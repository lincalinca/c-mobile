{
  "expo": {
    "name": "Crescender",
    "slug": "crescender-mobile",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "userInterfaceStyle": "dark",
    "description": "Track your music gear, lessons, and expenses by scanning receipts. Crescender automatically extracts gear items, lesson details, and transaction data from your receipts.",
    "extra": {
      "privacyPolicyUrl": "https://www.crescender.com.au/privacy",
      "router": {},
      "eas": {
        "projectId": "623de8c3-270e-4f7d-b2b0-f27b35433dc5"
      }
    },
    "splash": {
      "image": "./assets/900_1600.png",
      "resizeMode": "contain",
      "backgroundColor": "#2a0b4c"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.crescender.mobile.app",
      "buildNumber": "3",
      "displayName": "Crescender",
      "icon": "./assets/app-icon.png",
      "splash": {
        "image": "./assets/iPhone_14_-_1170_2536.png",
        "resizeMode": "contain",
        "backgroundColor": "#2a0b4c"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "Crescender needs access to your camera to scan receipts and extract gear information.",
        "NSUserTrackingUsageDescription": "This allows us to show you relevant ads and improve your experience.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "icon": "./assets/app-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png",
        "backgroundColor": "#2a0b4c"
      },
      "label": "Crescender",
      "splash": {
        "image": "./assets/Pixel_6_-_1080_2400.png",
        "resizeMode": "contain",
        "backgroundColor": "#2a0b4c"
      },
      "package": "com.crescender.mobile.app",
      "versionCode": 3,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "com.google.android.gms.permission.AD_ID",
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Crescender needs access to your camera to scan receipts."
        }
      ],
      "@react-native-community/datetimepicker",
      [
        "react-native-google-mobile-ads",
        {
          "iosAppId": "ca-app-pub-5375818323643018~2627507539",
          "androidAppId": "ca-app-pub-5375818323643018~6566752548"
        }
      ],
      "expo-tracking-transparency",
      [
        "expo-calendar",
        {
          "calendarPermission": "Crescender would like to add events to your calendar so you can view lessons and appointments next to your schedule."
        }
      ],
      "expo-font"
    ],
    "scheme": "crescender",
    "experiments": {
      "typedRoutes": true
    }
  }
}
